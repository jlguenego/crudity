{
  "name": "crudity",
  "version": "2.0.11",
  "description": "A production ready CRUD backend server and node http middleware, working with files/mongodb/etc.",
  "bin": "./build/src/bin/crudity.js",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "engines": {
    "node": ">=12"
  },
  "license": "ISC",
  "keywords": [
    "crud",
    "nodejs",
    "backend",
    "server",
    "http",
    "json",
    "express"
  ],
  "scripts": {
    "start": "npx nodemon src/bin/crudity.ts",
    "start:prod": "node build/src/bin/crudity.js",
    "test": "nyc mocha",
    "lint": "eslint .",
    "clean": "gts clean",
    "compile": "tsc",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "release": "standard-version",
    "build:schema": "typescript-json-schema --out schema/crudity.json ./src/interfaces/WebServerOptions.ts WebServerOptions"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.176",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/serve-index": "^1.9.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "got": "^11.8.2",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0",
    "standard-version": "^9.3.2",
    "ts-node": "^10.4.0",
    "typescript": "4.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "mariadb": "^3.0.0",
    "mongodb": "^4.1.4",
    "morgan": "^1.10.0",
    "qs": "^6.10.1",
    "rxjs": "^7.4.0",
    "serve-index": "^1.9.1",
    "typescript-json-schema": "^0.52.0",
    "yaml": "^1.10.2"
  },
  "author": {
    "email": "jlguenego@gmail.com",
    "name": "Jean-Louis GUENEGO",
    "url": "https://jlg-consulting.com"
  }
}
